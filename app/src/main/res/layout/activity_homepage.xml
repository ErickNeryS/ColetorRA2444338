<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DCA0A0"
    tools:context=".Homepage">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="34dp"
        android:text="Seja bem vindo"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textSize="48sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginBottom="12dp"
        android:text="Adicionar contagem"
        android:textColor="#754949"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/botao_novacontagem"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/botao_novacontagem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="39dp"
        android:clickable="true"
        app:backgroundTint="#CC7575"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:rippleColor="#752929"
        app:srcCompat="@android:drawable/ic_menu_add"
        android:onClick="openNovaContagemActivity"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:text="Total contado"
        android:textColor="#610213"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/botao_novacontagem" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="156dp"
        android:text="Ultima Contagem"
        android:textColor="#610213"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/botao_novacontagem" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Seção"
        android:textColor="#610213"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/txt_mostarultimasecao"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:text="Qtd. Contada"
        android:textColor="#610213"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_mostarultimaqtd"
        app:layout_constraintStart_toStartOf="@+id/txt_mostarultimaqtd" />

    <TextView
        android:id="@+id/txt_mostarultimasecao"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="31dp"
        android:text="TextView"
        android:textColor="#050202"
        android:textColorLink="#0B0303"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/txt_mostarultimaqtd"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/txt_mostarultimaqtd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="42dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/botao_novacontagem"
        app:layout_constraintTop_toTopOf="@+id/txt_mostarultimasecao" />

    <TextView
        android:id="@+id/txt_nomefuncionario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="7dp"
        android:text="Nome funcionario"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:text="@string/txt_nomefuncionario" /><![CDATA[





    app:borderWidth="4dp"
    app:borderTint="#5C4033"
    tools:layout_editor_absoluteX="238dp"
    tools:layout_editor_absoluteY="142dp" />



]]>
</androidx.constraintlayout.widget.ConstraintLayout>